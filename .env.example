# FinnHub API 密钥，用于获取美股或数字货币行情
FINNHUB_API_KEY=your_finnhub_api_key

# 如需使用 Alpha Vantage，可在此处填写密钥；不使用则留空或保留占位符
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key

# DeepSeek API 密钥，用于调用 DeepSeek 模型
DEEPSEEK_API_KEY=your_deepseek_api_key

# 将 OPENAI_API_KEY 设置为您的 DeepSeek 密钥，让 ChatOpenAI 使用 DeepSeek
OPENAI_API_KEY=your_deepseek_api_key
# 指定 DeepSeek 的 API 端点，而不是默认的 OpenAI 端点
OPENAI_API_BASE=https://api.deepseek.com
# 某些版本的 SDK 使用 OPENAI_BASE_URL，也同时设置
OPENAI_BASE_URL=https://api.deepseek.com

# 如需使用 OpenAI，请在此处填写 OpenAI 的 API 密钥
OPENAI_API_KEY_FOR_OPENAI=your_openai_api_key
# OpenAI 默认接口地址，可根据需要调整
OPENAI_API_BASE_FOR_OPENAI=https://api.openai.com/v1
